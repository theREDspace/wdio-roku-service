{
    "name": "wdio-roku-service",
    "version": "1.0.0",
    "description": "WebdriverIO service to facilitate Roku testing",
    "author": "REDspace",
    "homepage": "https://www.redspace.com",
    "license": "",
    "type": "module",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "rm -rf dist/ && tsc --removeComments && tsc --declaration --emitDeclarationOnly"
    },
    "keywords": [
        "webdriverio",
        "wdio",
        "wdio-service",
        "roku"
    ],
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        },
        "./controller": {
            "import": "./dist/controller.js",
            "types": "./dist/controller.d.ts"
        },
        "./info": {
            "import": "./dist/info.js",
            "types": "./dist/info.d.ts"
        },
        "./channel": {
            "import": "./dist/channel.js",
            "types": "./dist/channel.d.ts"
        },
        "./ecp": {
            "import": "./dist/ecp.js",
            "types": "./dist/ecp.d.ts"
        },
        "./install": {
            "import": "./dist/install.js",
            "types": "./dist/install.d.ts"
        }
    },
    "dependencies": {
        "@wdio/globals": "^9.0.9",
        "@wdio/utils": "^9.1.0",
        "expect": "^29.7.0",
        "expect-webdriverio": "^5.0.2",
        "tmp": "^0.2.3",
        "webdriverio": "^9.0.9"
    },
    "devDependencies": {
        "@eslint/js": "^9.10.0",
        "@types/eslint-config-prettier": "^6.11.2",
        "@types/node": "^22.5.5",
        "@types/tmp": "^0.2.6",
        "@typescript-eslint/eslint-plugin": "^8.4.0",
        "@typescript-eslint/parser": "^8.4.0",
        "@wdio/types": "^9.0.8",
        "eslint": "^9.10.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-import-resolver-typescript": "^3.6.3",
        "eslint-plugin-import-x": "^4.2.1",
        "prettier": "^3.3.3",
        "typescript": "^5.6.2"
    },
    "files": [
        "dist/*"
    ]
}
